# Test for loops with break and continue

def test_sum() -> int:
    let sum: int = 0
    for i in range(1, 6):
        sum = sum + i
    return sum

def test_break() -> int:
    let last: int = 0
    for i in range(0, 10):
        if i == 5:
            break
        last = i
    return last

def test_continue() -> int:
    let sum: int = 0
    for i in range(0, 6):
        if i == 3:
            continue
        sum = sum + i
    return sum

def main() -> int:
    let s: int = test_sum()
    print(s)
    let b: int = test_break()
    print(b)
    let c: int = test_continue()
    print(c)
    return 0
